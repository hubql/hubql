{    
    "$schema": "https://turborepo.org/schema.json",
       
    "tasks": {
        "setup": {
            "cache": false,
            "dependsOn": [
                "^build"
            ]
        },
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                "storybook-static/**"
            ]
        },
        "docs#build": {
            "dependsOn": [
                "^build"
            ],
            "env": [
                "ANALYZE",
                "NODE_ENV"
            ],
            "inputs": [
                "$TURBO_DEFAULT$"
            ],
            "outputs": []
        },
        "lint": {
            "outputs": []
        },
        "clean": {
            "outputs": [],
            "cache": false
        },
        "dev": {
            "persistent": true,
            "cache": false
        },
        "test": {
            "cache": false
        },
        "typecheck": {
            "dependsOn": [
                "^typecheck"
            ],
            "outputs": [
                "**/node_modules/.cache/tsbuildinfo.json"
            ]
        }
    }
}